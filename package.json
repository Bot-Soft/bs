{
  "name": "bs",
  "description": "Command Line Interface for Managing BotSoft chatbots",
  "version": "0.0.0",
  "author": "Angel Tsvetkov @angeltsvetkov",
  "bin": {
    "bs": "./bin/run"
  },
  "bugs": "https://github.com/Bot-Soft/bs/issues",
  "dependencies": {
    "@oclif/command": "^1.4.34",
    "@oclif/config": "^1.6.33",
    "@oclif/plugin-help": "^2.0.5",
    "@types/fs-extra": "^5.0.3",
    "@types/request": "^2.47.1",
    "fs-extra": "^6.0.1",
    "request": "^2.83.0",
    "tslib": "^1.9.3",
    "typed-rest-client": "^1.0.7"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.15.4",
    "@oclif/test": "^1.1.0",
    "@oclif/tslint": "^1.1.2",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.3",
    "@types/node": "^10.3.6",
    "chai": "^4.1.2",
    "globby": "^8.0.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "ts-node": "^6.2.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Bot-Soft/bs",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "bs",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Bot-Soft/bs",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "rm -rf lib && tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
